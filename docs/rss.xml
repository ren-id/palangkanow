<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:sy="http://purl.org/rss/1.0/modules/syndication/" xmlns:slash="http://purl.org/rss/1.0/modules/slash/" version="2.0">
<channel>
<title>Palangka Now</title>
<description>Palangka Now (palangkanow) adalah blog publik warga Palangka Raya. Jika Anda warga Palangka Raya, maka Anda dapat berpartisipasi.</description>
<link>{{ site.url }}</link>
<language>id</language>
<copyright>Palangka Now</copyright>
<atom:link href="{{ site.url }}/rss.xml" rel="self" type="application/rss+xml"/>
{% for post in site.posts %}
<item>
<title>{{ post.title }}</title>
<link>{{ site.url }}{{ post.url }}</link>
<pubDate>{{ post.date }}</pubDate>
<description>{{ post.description }}</description>
<guid isPermaLink="false">{{ site.url }}{{ post.url }}</guid>
</item>
{% endfor %}
</channel>
</rss>
